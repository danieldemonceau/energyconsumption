{
  "name": "energy-consumption-api",
  "version": "1.0.0",
  "description": "Energy Consumption API",
  "main": "src/index.ts",
  "scripts": {
    "dev": "nodemon src/index.ts",
    "dev:debug": "nodemon --inspect src/index.ts",
    "start": "node --inspect=5858 -r ts-node/register ./src/index.ts",
    "start:dev": "node build",
    "start:watch": "nodemon ./src/index.ts",
    "build": "babel src --out-dir build --extensions .ts,.tsx --config-file ./babel.config.json",
    "build:start:dev": "babel src --out-dir build --extensions .ts,.tsx --config-file ./babel.config.json && nodemon ./build/",
    "build:start:prod": "babel src --out-dir build --extensions .ts,.tsx --config-file ./babel.config.json && node ./build/",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "tslint-check": "tslint-config-prettier-check",
    "test": "jest --coverage",
    "test:watch": "jest --coverage --watch",
    "test:debug": "node --inspect-brk ./node_modules/jest/bin/jest -i",
    "check-types": "tsc"
  },
  "author": "Daniel Demonceau",
  "license": "MIT",
  "dependencies": {
    "@types/express": "^4.17.9",
    "@types/multer": "^1.4.4",
    "@types/pg": "^7.14.7",
    "@types/validator": "^13.1.0",
    "app-root-path": "^3.0.0",
    "chokidar": "^3.5.1",
    "current-git-branch": "^1.1.0",
    "express": "^4.17.1",
    "fast-csv": "^4.3.5",
    "helmet": "^4.4.1",
    "multer": "^1.4.2",
    "nodemon": "^2.0.6",
    "pg": "^8.4.2",
    "supertest": "^6.1.3",
    "validator": "^13.1.17",
    "winston": "^3.3.3"
  },
  "devDependencies": {
    "@babel/cli": "^7.12.7",
    "@babel/core": "^7.12.7",
    "@babel/plugin-proposal-class-properties": "^7.12.1",
    "@babel/plugin-proposal-object-rest-spread": "^7.12.1",
    "@babel/preset-env": "^7.12.7",
    "@babel/preset-typescript": "^7.12.7",
    "@babel/register": "^7.12.1",
    "@types/app-root-path": "^1.2.4",
    "@types/current-git-branch": "^1.1.1",
    "@types/jest": "^26.0.15",
    "@types/node": "^14.14.9",
    "@types/supertest": "^2.0.10",
    "@typescript-eslint/eslint-plugin": "^4.20.0",
    "@typescript-eslint/parser": "^4.20.0",
    "babel-cli": "^6.18.0",
    "babel-jest": "^26.6.3",
    "babel-preset-env": "^1.7.0",
    "babel-preset-stage-0": "^6.24.1",
    "chalk": "^4.1.0",
    "colorize": "^0.1.0",
    "eslint": "^7.23.0",
    "eslint-config-airbnb": "^18.2.1",
    "eslint-config-airbnb-base": "^14.2.1",
    "eslint-config-prettier": "^8.1.0",
    "eslint-import-resolver-typescript": "^2.4.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-json": "^2.1.2",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-prettier": "^3.3.1",
    "eslint-plugin-react": "^7.23.1",
    "husky": "^4.3.0",
    "jest": "^26.6.3",
    "lint-staged": "^10.4.2",
    "prettier": "^2.2.1",
    "superagent": "^6.1.0",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "^4.1.2",
    "typescript-eslint": "0.0.1-alpha.0"
  },
  "jest": {
    "testMatch": [
      "**/dist/**/?(*.)(spec|test).js?(x)"
    ],
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "json"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "pre-push": "lint-staged"
    }
  },
  "lint-staged": {
    "*.js": "eslint --cache --fix",
    "*.ts": "eslint --cache --fix",
    "**/*": "prettier --write --ignore-unknown"
  }
}
