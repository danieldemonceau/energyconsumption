{
  "repository": {
    "type": "git",
    "url": "git://github.com/danieldemonceau/energyconsumption.git"
  },
  "name": "energy-consumption-api",
  "version": "1.0.0",
  "description": "Energy Consumption API",
  "main": "src/index.ts",
  "scripts": {
    "dev": "nodemon src/index.ts",
    "dev:debug": "nodemon --inspect src/index.ts",
    "start": "node --inspect=5858 -r ts-node/register ./src/index.ts",
    "start:dev": "node build",
    "start:watch": "nodemon ./src/index.ts",
    "build": "babel src --out-dir build --extensions .ts,.tsx --config-file ./babel.config.json",
    "build:start:dev": "babel src --out-dir build --extensions .ts,.tsx --config-file ./babel.config.json && nodemon ./build/",
    "build:start:prod": "babel src --out-dir build --extensions .ts,.tsx --config-file ./babel.config.json && node ./build/",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "tslint-check": "tslint-config-prettier-check",
    "test": "jest --coverage",
    "test:watch": "jest --coverage --watch",
    "test:debug": "node --inspect-brk ./node_modules/jest/bin/jest -i",
    "check-types": "tsc"
  },
  "author": "Daniel Demonceau",
  "license": "MIT",
  "dependencies": {
    "@types/express": "^4.17.13",
    "@types/multer": "^1.4.7",
    "@types/pg": "^8.6.1",
    "@types/validator": "^13.6.3",
    "app-root-path": "^3.0.0",
    "braces": "^3.0.2",
    "chokidar": "^3.5.2",
    "current-git-branch": "^1.1.0",
    "encodeurl": "^1.0.2",
    "express": "^4.17.1",
    "fast-csv": "^4.3.6",
    "glob-parent": "^6.0.1",
    "helmet": "^4.6.0",
    "multer": "^1.4.3",
    "nodemon": "^2.0.13",
    "pg": "^8.7.1",
    "request": "^2.88.2",
    "supertest": "^6.1.6",
    "validator": "^13.6.0",
    "winston": "^3.3.3"
  },
  "devDependencies": {
    "@babel/cli": "^7.14.8",
    "@babel/core": "^7.15.0",
    "@babel/plugin-proposal-class-properties": "^7.14.5",
    "@babel/plugin-proposal-object-rest-spread": "^7.14.7",
    "@babel/preset-env": "^7.15.0",
    "@babel/preset-typescript": "^7.15.0",
    "@babel/register": "^7.15.3",
    "@types/app-root-path": "^1.2.4",
    "@types/current-git-branch": "^1.1.2",
    "@types/encodeurl": "^1.0.0",
    "@types/jest": "^27.0.1",
    "@types/node": "^16.7.2",
    "@types/request": "^2.48.7",
    "@types/supertest": "^2.0.11",
    "@typescript-eslint/eslint-plugin": "^4.29.3",
    "@typescript-eslint/parser": "^4.29.3",
    "axios": "^0.21.1",
    "babel-cli": "^6.26.0",
    "babel-jest": "^27.1.0",
    "babel-preset-env": "^1.7.0",
    "babel-preset-stage-0": "^6.24.1",
    "chalk": "^4.1.2",
    "colorize": "^0.1.0",
    "eslint": "^7.32.0",
    "eslint-config-airbnb": "^18.2.1",
    "eslint-config-airbnb-base": "^14.2.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-import-resolver-typescript": "^2.4.0",
    "eslint-plugin-import": "^2.24.2",
    "eslint-plugin-json": "^3.1.0",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-prettier": "^3.4.1",
    "eslint-plugin-react": "^7.24.0",
    "eslint-plugin-react-hooks": "^4.2.0",
    "form-data": "^4.0.0",
    "husky": "^7.0.2",
    "jest": "^27.1.0",
    "lint-staged": "^11.1.2",
    "prettier": "^2.3.2",
    "superagent": "^6.1.0",
    "ts-node": "^10.2.1",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "^4.4.2",
    "typescript-eslint": "0.0.1-alpha.0"
  },
  "jest": {
    "testMatch": [
      "**/dist/**/?(*.)(spec|test).js?(x)"
    ],
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "json"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "pre-push": "lint-staged"
    }
  },
  "lint-staged": {
    "*.js": "eslint --cache --fix",
    "*.ts": "eslint --cache --fix",
    "**/*": "prettier --write --ignore-unknown"
  }
}
